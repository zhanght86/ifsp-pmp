*{
	margin: 0;
	padding: 0;
}
body{
	background-color:#9FF6F0;
	background: url("../imgs/img_bg.jpg")no-repeat 0 -5px;
	font-family: "寰蒋闆呴粦";
	width: 100%;
}

.container{
	width: 1000px;
	margin: 0 auto;
}
a,a:hover,a:active,a:focus,a:visited{
	border: none 0;
	outline: none 0;
	text-decoration: none;
}
.header{
/* 	border-bottom: 1px solid #eee; */
/* 	background: white; */
/*  background: url("../imgs/header.png") no-repeat left center; */

}
.header .container{
	height: 100px;
	line-height: 100px;
/* 	background: url("../imgs/rx_logo.png") no-repeat left center; */
/*     background: url("../imgs/zz_bank_logo.png") no-repeat right center; */
}
.header .link{
	float: right;
	font-size: 40px;
	font-family: "华文楷体";
	font-weight:bolder;
	color: #E30012;
}
.header .link a,.header .link span{
	display: inline-block;
	color: #29c7da;
	font-size: 40px;	
}
.header .link a:hover{
	text-decoration: underline;
}
.header .link span{
	margin: 0 6px;
}
