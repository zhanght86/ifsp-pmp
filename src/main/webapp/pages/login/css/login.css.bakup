.center .form-div{
	margin-top: 130px;
	position: absolute;
	right: 60px;
	border: 1px solid #eee;
/* 	background: white; */
	background: rgba(255,255,255, 0.8);
}
.center .form{
	width: 300px;
	margin: 0 40px 40px 40px;
}
