.center .form .divh3 h3{
	margin-left:120px;
	height: 70px;
	line-height: 70px;
	background-position:center center;
	background: url("../imgs/login.png") no-repeat 0 10px;
}


.center .form div{
	margin: 20px 0;
	position: relative;
}
.center .form input{
	width: 292px;
	height: 38px;
	vertical-align: middle;
	line-height: 40px\0;
	*line-height: 40px;
	font-size: 14px;
	color: #666;
	border: 1px solid #ddd;
	outline: none;
	padding-left: 6px;
}
.center .form i{
	display: block;
	height: 40px;
	width: 40px;
	position: absolute;
	top: 0;
	right: 0;
}
.center i.icon-user{
	background: url("../imgs/login-icon.gif") no-repeat 0 top;
}
.center i.icon-pwd{
	background: url("../imgs/login-icon.gif") no-repeat 0 -40px;
}
.center .ifocus i.icon-user{
	background: url("../imgs/login-icon.gif") no-repeat 0 -80px;
}
.center .ifocus i.icon-pwd{
	background: url("../imgs/login-icon.gif") no-repeat 0 -120px;
}
.center input:hover{
	border: 1px solid #ccc;
}
.center .ifocus input{
	border: 1px solid #29c7da;
}
.center .btn-div a{
	display: block;
	width: 300px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: white;
/* 	background: #29c7da; */
	background: #F17B00;
	border: none 0;
	font-size: 16px;
	border-radius: 5px;
	cursor: pointer;
}
.center .btn-div a:hover{
	text-decoration: underline;
	opacity: 0.85;
	filter: alpha(opacity=85);
}
.footer{
/* 	background: white; */
/* 	border-top: 1px solid #eee; */
	width: 100%;
	position: fixed;
	left:0;
	bottom: 0;  /* 闈濱E涓嬬殑瀹氫綅 */
	_position: absolute;  /*IE6 鐢╝bsolute妯℃嫙fixed*/
	_top: expression(document.documentElement.scrollTop + Math.round(document.documentElement.offsetHeight-81) + "px"); /*IE6 鍔ㄦ�乼op鍊�*/
	right:0;	
}
.footer p{
	height: 80px;
	line-height: 80px;
	text-align: center;
	color: #FFF;
	font-size: 20px;
	font-family: "华文楷体";
	font-weight:bolder;
}


