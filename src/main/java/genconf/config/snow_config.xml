<?xml version="1.0" encoding="UTF-8"?>
<!-- 系统全局配置，必须设置 -->
<contextParams>
	<contextParam>
	     <!--是否为生产模式-->
	    <paramName>productmode</paramName>
	    <paramValue>true</paramValue>
	</contextParam>
	<contextParam>
	    <!--spring事务管理器名称(默认为空使用框架自带事务管理器)-->
	    <paramName>transcationManagerName</paramName>
	    <paramValue></paramValue>
	</contextParam>
	<contextParam>
	    <!--国际化默认语言(zh_CN)-->
	    <paramName>language</paramName>
	    <paramValue>zh_CN</paramValue>
	</contextParam>
	<contextParam>
	    <!--是否从(监控)数据库加载-->
	    <paramName>dbload</paramName>
	    <paramValue>false</paramValue>
	</contextParam>
	<contextParam>
	     <!--是否启动远程治理服务(需使用分布式部署)-->
	    <paramName>monitor</paramName>
	    <paramValue>false</paramValue>
	</contextParam>
	<contextParam>
	    <!--是否记录交易信息(需配置管理数据源)-->
	    <paramName>savetradvol</paramName>
	    <paramValue>false</paramValue>
	</contextParam>
	<contextParam>
	     <!--是否启用分布式部署-->
	    <paramName>distributed</paramName>
	    <paramValue>false</paramValue>
	</contextParam>
	<contextParam>
	    <!--初始化log对象默认配置路径-->
	    <paramName>logconfig</paramName>
	    <paramValue>/coreconf/snow_log.xml</paramValue>
	</contextParam>
	<contextParam>
	    <!--全局日志记录目录-->
	    <paramName>logpath</paramName>
	    <paramValue>/ztsoft/usr/wasadm/logs/ifsp-pmp</paramValue>
	</contextParam>
	<contextParam>
	     <!--全局日志级别，默认INFO-->
	    <paramName>loglevel</paramName>
	    <paramValue>debug</paramValue>
	</contextParam>
	<contextParam>
	    <!--通道及服务执行线程池大小(为空，默认cpu核数+1)-->
	    <paramName>serverThreadPool</paramName>
	    <paramValue>20</paramValue>
	</contextParam>
</contextParams>